{
    "scripts": {
        "client": "cd app && npm run start",
        "server": "cd server && npm run start",
        "start": "concurrently \"npm run server\" \"npm run client\"",
        "server_dev": "cd server && npm run dev",
        "dev": "concurrently \"npm run server_dev\" \"npm run client\"",
        "install-client": "cd app && npm install",
        "build-client": "cd app && npm run build"
    },
    "devDependencies": {
        "concurrently": "^8.2.0"
    },
    "dependencies": {
        "@emailjs/browser": "^3.11.0"
    }
}
